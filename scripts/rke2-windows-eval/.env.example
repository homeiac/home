# RKE2 Windows Eval Environment Variables
# Copy to .env and fill in values

# Windows VM credentials
WINDOWS_VM_IP=192.168.4.201
WINDOWS_USER=Administrator
WINDOWS_PASSWORD=your_password_here

# Proxmox jump host
PROXMOX_HOST=pumped-piglet.maas

# VM IPs
RANCHER_VM_IP=192.168.4.200
LINUX_VM_IP=192.168.4.202

# Rancher settings
RANCHER_HOSTNAME=rancher.homelab

# Cluster registration (get from Rancher UI after creating cluster)
# These are in the registration command: --token XXX --ca-checksum YYY
RANCHER_TOKEN=UPDATE_ME
RANCHER_CA_CHECKSUM=UPDATE_ME
