# Office RKE2 Evaluation Environment
# Copy to .env and fill in values

# Proxmox host
PROXMOX_HOST=pve.office.local

# Entra ID / Azure AD (from App Registration)
TENANT_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
CLIENT_ID=yyyyyyyy-yyyy-yyyy-yyyy-yyyyyyyyyyyy
CLIENT_SECRET=your-client-secret-value

# Rancher VM
RANCHER_IP=192.168.1.200
RANCHER_HOSTNAME=rancher.office.local

# Windows VM
WINDOWS_IP=192.168.1.201
WINDOWS_USER=Administrator
WINDOWS_PASSWORD=YourWindowsPassword

# Rancher registration (get from Rancher UI after cluster creation)
RANCHER_TOKEN=xxxxxx::yyyyyy
RANCHER_CA_CHECKSUM=abc123def456...
